# Flowbiz API Configuration
FLOWBIZ_ENDPOINT=https://mbiz.mailclick.me/api.php
FLOWBIZ_API_KEY_Voxcall=YOUR_FLOWBIZ_API_KEY_HERE
FLOWBIZ_METHOD_PARAM=Command
FLOWBIZ_RESPONSE_FORMAT=JSON
FLOWBIZ_APPEND_METHOD_PATH=false
FLOWBIZ_TIMEOUT_SECONDS=20

# Database Configuration (PostgreSQL)
DB_HOST=your_database_host
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password

# Application Settings (optional)
FLASK_ENV=development
FLASK_DEBUG=0
